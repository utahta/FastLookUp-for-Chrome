{
    "name": "FastLookUp for Chrome",
    "version": "0.1.2",
    "description": "A translator extension for japanese. like fastlookupalc.",
    "options_page": "options.html",
    "background_page": "background.html",
    "permissions": [
        "tabs",
        "http://*/*"
    ],
    "content_scripts": [
        {
        "matches": ["http://*/*","https://*/*"],
        "js": ["fastlookup.js"]
        }
    ],
    "update_url": "http://github.com/yuta/FastLookUp-for-Chrome/raw/master/updates.xml"
}
