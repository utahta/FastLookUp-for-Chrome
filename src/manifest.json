{
    "name": "FastLookUp",
    "version": "1.1.6",
    "description": "A translator extension for chrome.",
    "options_page": "options.html",
    "background_page": "background.html",
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "icons": { 
        "16": "16.png",
	    "32": "32.png",
	    "48": "48.png",
	    "128": "128.png"
    },
    "default_locale": "en"
}
