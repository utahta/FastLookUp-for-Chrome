{
    "name": "FastLookUp",
    "version": "0.1",
    "description": "A translator extension for japanese. like fastlookupalc.",
    "options_page": "options.html",
    "background_page": "background.html",
    "permissions": [
        "tabs",
        "http://*/*"
    ],
    "content_scripts": [
        {
        "matches": ["http://*/*","https://*/*"],
        "js": ["fastlookup.js"]
        }
    ]
}
